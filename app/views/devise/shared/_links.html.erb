<%- if controller_name != "sessions" %>
  <%= link_to t("sessions.new"), new_session_path(resource_name), class: "btn btn-link mt-1 mymargin" %><br/>
<% end %>

<%- if devise_mapping.registerable? && controller_name != "registrations" %>
  <%= link_to t("registrations.new"), new_registration_path(resource_name), class: "btn btn-link mt-1 mymargin" %><br/>
<% end %>

<%- if devise_mapping.recoverable? && controller_name != "passwords" && controller_name != "registrations" %>
  <%= link_to t("devise.passwords.new"), new_password_path(resource_name), class: "btn btn-link mt-1 mymargin" %><br/>
<% end %>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= button_to "#{t("button.sign_in_with")} #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), data: { turbo: false }, class: "btn btn-link mt-1 mymargin" %>
  <% end %>
<% end %>
